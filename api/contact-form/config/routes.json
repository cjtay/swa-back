{
  "routes": [
    {
      "method": "GET",
      "path": "/contact-forms",
      "handler": "contact-form.find",
      "config": {
        "policies": ["global::domainPolicy"]
      }
    },
    {
      "method": "GET",
      "path": "/contact-forms/count",
      "handler": "contact-form.count",
      "config": {
        "policies": ["global::domainPolicy"]
      }
    },
    {
      "method": "GET",
      "path": "/contact-forms/:id",
      "handler": "contact-form.findOne",
      "config": {
        "policies": ["global::domainPolicy"]
      }
    },
    {
      "method": "POST",
      "path": "/contact-forms",
      "handler": "contact-form.create",
      "config": {
        "policies": ["global::domainPolicy"]
      }
    },
    {
      "method": "PUT",
      "path": "/contact-forms/:id",
      "handler": "contact-form.update",
      "config": {
        "policies": ["global::domainPolicy"]
      }
    },
    {
      "method": "DELETE",
      "path": "/contact-forms/:id",
      "handler": "contact-form.delete",
      "config": {
        "policies": ["global::domainPolicy"]
      }
    }
  ]
}
